{
  "properties": {
    "enableAcceleratedNetworking": false,
    "ipConfigurations": [
      {
        "name": "{{Name}}",
        "properties": {
          "publicIPAddress": {
            "id": "/subscriptions/{{SubscriptionId}}/resourceGroups/{{ResourceGroup}}/providers/Microsoft.Network/publicIPAddresses/{{IpAddressId}}"
          },
          "subnet": {
            "id": "/subscriptions/{{SubscriptionId}}/resourceGroups/{{ResourceGroup}}/providers/Microsoft.Network/virtualNetworks/{{VirtualNetwork}}/subnets/default"
          }
        }
      }
    ],
    "networkSecurityGroup": {
      "id": "/subscriptions/{{SubscriptionId}}/resourceGroups/{{ResourceGroup}}/providers/Microsoft.Network/networkSecurityGroups/{{NetworkSecurityGroup}}"
    }
  },
  "location": "{{Location}}"
}